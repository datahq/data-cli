{
  "name": "datahub-cli",
  "version": "0.1.0",
  "description": "CLI for working with data packages",
  "main": "./lib/index.js",
  "bin": {
    "data": "./bin/data.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ava -v",
    "watch:test": "npm t -- --watch",
    "data": "node bin/data.js",
    "pack": "pkg bin/data.js -c package.json -o packed/datahub"
  },
  "ava": {
    "failFast": true,
    "files": [
      "test/test_*.js"
    ]
  },
  "pkg": {
    "scripts": [
      "bin/*",
      "lib/**/*"
    ],
    "targets": [
      "node7-linux-x64",
      "node7-macos-x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datopian/datahub-cli.git"
  },
  "keywords": [
    "datahub",
    "datapackage"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/datopian/datahub-cli/issues"
  },
  "homepage": "https://github.com/datopian/datahub-cli#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "csv-parse": "^1.2.0",
    "datapackage": "^1.0.0-alpha.4",
    "expand-home-dir": "0.0.3",
    "ini": "^1.3.4",
    "marked": "^0.3.6",
    "marked-terminal": "^2.0.0",
    "minimist": "^1.2.0",
    "ora": "^1.2.0",
    "progress": "^2.0.0",
    "prompt": "^1.0.0",
    "tableschema": "^1.0.0-alpha.7",
    "tmp": "0.0.31",
    "url-join": "^2.0.2"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "cross-spawn": "^5.1.0",
    "mocha": "^3.4.1",
    "nock": "^9.0.13",
    "pkg": "^3.0.6",
    "sinon": "^2.3.2"
  }
}
